#include <unistd.h>
/**
*_putchar -writes the character to c stdout
*$c:the chracter to print
*return:on success 1.
*on error,-1 is returned,and errors are set appropriately.
*/
int_putchar(char c)
{
return (write(1,&c,1));
}
int _putchar(char);
